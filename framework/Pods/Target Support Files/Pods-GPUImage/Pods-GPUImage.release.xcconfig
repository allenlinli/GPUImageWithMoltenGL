FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/MoltenGL/MoltenGL/iOS" "${PODS_ROOT}/MoltenGL/MoltenGLShaderConverter/iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MoltenGL"
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -framework "GLKit" -framework "Metal" -framework "MoltenGL" -framework "MoltenGLShaderConverter" -framework "OpenGLES" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
VALID_ARCHS = arm64 armv7
