ALWAYS_SEARCH_USER_PATHS = YES
ARCHS = arm64 armv7
CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
CLANG_ENABLE_MODULES = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/MoltenGL
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/MoltenGL/MoltenGL/iOS" "${PODS_ROOT}/MoltenGL/MoltenGLShaderConverter/iOS" MoltenGL/iOS MoltenGLShaderConverter/iOS
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/MoltenGL" "${PODS_ROOT}/Headers/Public"
ONLY_ACTIVE_ARCH = YES
OTHER_LDFLAGS = $(inherited) -l"ObjC" -l"c++" -framework "GLKit" -framework "Metal" -framework "OpenGLES" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/MoltenGL
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
SWIFT_VERSION = 4.0
USER_HEADER_SEARCH_PATHS = MoltenGL/RedirectHeaders/include/**/*
VALID_ARCHS = arm64 armv7
VALID_ARCHS[sdk=iphonesimulator*] =
